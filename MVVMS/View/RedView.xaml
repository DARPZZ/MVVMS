<UserControl x:Class="MVVMS.View.RedView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MVVMS.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <StackPanel>
        <TextBox Text="{Binding ID}" Width="300" Height="100" FontSize="35"/>
        <Button x:Name="AddText" Width="200" Content="Add Text" Command="{Binding AddTextCMD}"/>
        <Button x:Name="ChangePage" Width="200" Content="Change Page" Command="{Binding ChangePageCMD}"/>
    </StackPanel>

</UserControl>
